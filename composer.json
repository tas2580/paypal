{
	"name": "tas2580/paypal",
	"type": "phpbb-extension",
	"description": "Adds a page to donate with Paypal",
	"homepage": "https://tas2580.net/downloads/phpbb-paypal/",
	"version": "0.1.0",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "tas2580",
			"email": "mail@tas2580.net",
			"homepage": "https://tas2580.net",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Paypal donations",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.4,<3.2.0@dev"
		},
		"version-check": {
			"host": "phpbb-versionscheck.cf",
			"directory": "/tas2580/paypal/master",
			"filename": "version-check.json"
		}
	}
}
